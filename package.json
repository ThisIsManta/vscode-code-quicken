{
    "name": "code-quicken",
    "displayName": "Manta's Code Quicken",
    "description": "Insert import/require statement quickly.",
    "version": "0.0.10",
    "publisher": "thisismanta",
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "import",
        "require",
        "javascript",
        "file",
        "module"
    ],
    "repository": {
        "url": "https://github.com/ThisIsManta/vscode-code-quicken"
    },
    "bugs": {
        "url": "https://github.com/ThisIsManta/vscode-code-quicken/issues"
    },
    "icon": "docs/logo.png",
    "galleryBanner": {
        "color": "#ffea00",
        "theme": "light"
    },
    "engines": {
        "vscode": "^1.12.0"
    },
    "activationEvents": [
        "onCommand:codeQuicken.addImport",
        "onCommand:codeQuicken.fixImport"
    ],
    "main": "./dist/extension",
    "contributes": {
        "commands": [
            {
                "command": "codeQuicken.addImport",
                "title": "Code Quicken: Insert an import/require statement"
            },
            {
                "command": "codeQuicken.fixImport",
                "title": "Code Quicken: Fix broken import/require statements"
            }
        ],
        "configuration": {
            "title": "Code Quicken",
            "properties": {
                "codeQuicken.rememberLastSelection": {
                    "type": "integer",
                    "minimum": 0,
                    "default": 3
                },
                "codeQuicken.javascript.preferImports": {
                    "type": "boolean",
                    "default": false
                },
                "codeQuicken.javascript.removeFileExtension": {
                    "type": "boolean",
                    "default": false
                },
                "codeQuicken.javascript.removeIndexFile": {
                    "type": "boolean",
                    "default": false
                },
                "codeQuicken.javascript.removeSemiColons": {
                    "type": "boolean",
                    "default": false
                },
                "codeQuicken.javascript.preferSingleQuotes": {
                    "type": "boolean",
                    "default": false
                },
                "codeQuicken.javascript.groupImports": {
                    "type": "boolean",
                    "default": false
                },
                "codeQuicken.javascript.variableNamingConvention": {
                    "type": "string",
                    "enum": [
                        "camelCase",
                        "snake_case",
                        "lowercase",
                        "as-is"
                    ],
                    "default": "as-is"
                },
                "codeQuicken.javascript.predefinedVariableNames": {
                    "type": "object",
                    "patternProperties": {
                        ".": {
                            "type": "string"
                        }
                    },
                    "default": {}
                },
                "codeQuicken.javascript.filteredFileNames": {
                    "type": "object",
                    "patternProperties": {
                        ".": {
                            "type": "string"
                        }
                    },
                    "default": {}
                }
            }
        },
        "keybindings": [
            {
                "command": "codeQuicken.addImport",
                "key": "ctrl+shift+i"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "compile": "tsc --watch --project ./",
        "vscode:prepublish": "tsc --skipLibCheck --project ./",
        "version": "vsce publish",
        "postversion": "git push --tags --quiet origin master:master"
    },
    "devDependencies": {
        "@types/babylon": "^6.16.2",
        "@types/lodash": "^4.14.73",
        "@types/node": "^6.0.86",
        "typescript": "~2.5.3",
        "vscode": "^1.1.4"
    },
    "dependencies": {
        "babylon": "^6.18.0",
        "lodash": "^4.17.4"
    }
}